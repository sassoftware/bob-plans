<?xml version='1.0' encoding='utf-8'?>
<project>
  <actions />
  <description />
  <keepDependencies>false</keepDependencies>
  <properties />
  <scm class="org.jenkinsci.plugins.git_wms.GitWMSAgent" plugin="git-wms@1.0-SNAPSHOT">
    <path>gerrit-pdt/appengine/appengine</path>
    <branch>@BRANCH@</branch>
    <watchPaths>
      <org.jenkinsci.plugins.git__wms.GitWMSAgent_-WatchPath>
        <watchRepo>gerrit-pdt/appengine/recipes</watchRepo>
        <watchPath>product-definitions</watchPath>
      </org.jenkinsci.plugins.git__wms.GitWMSAgent_-WatchPath>
    </watchPaths>
  </scm>
  <quietPeriod>15</quietPeriod>
  <canRoam>true</canRoam>
  <disabled>false</disabled>
  <blockBuildWhenDownstreamBuilding>false</blockBuildWhenDownstreamBuilding>
  <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
  <triggers>
    <hudson.triggers.SCMTrigger>
      <spec>@hourly</spec>
      <ignorePostCommitHooks>false</ignorePostCommitHooks>
    </hudson.triggers.SCMTrigger>
  </triggers>
  <concurrentBuild>false</concurrentBuild>
  <builders>
    <hudson.tasks.Shell>
      <command>
for product in rba rus devimage rwbs; do
 for target in "" "-infra"; do
  bob-jenkins --base-uri $WMS --repo gerrit-pdt/appengine/bob-plans --plan centos-6n/product-definition-${product}${target}.bob
 done
done

bob-jenkins --base-uri $WMS --repo gerrit-pdt/appengine/bob-plans --plan centos-6n/product-definition-rba-vapp.bob
bob-jenkins --base-uri $WMS --repo gerrit-pdt/appengine/bob-plans --plan centos-6n/product-definition-entsrv.bob
</command>
    </hudson.tasks.Shell>
  </builders>
  <publishers />
  <buildWrappers />
</project>
