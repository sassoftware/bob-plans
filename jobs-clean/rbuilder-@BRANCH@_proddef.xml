<?xml version='1.0' encoding='utf-8'?>
<project>
  <actions />
  <description />
  <keepDependencies>false</keepDependencies>
  <properties />
  <scm class="org.jenkinsci.plugins.git_wms.GitWMSAgent">
    <path>scc/appengine</path>
    <branch>@BRANCH@</branch>
    <watchPaths>
      <org.jenkinsci.plugins.git__wms.GitWMSAgent_-WatchPath>
        <watchRepo>scc/rpath-product-definition</watchRepo>
        <watchPath />
      </org.jenkinsci.plugins.git__wms.GitWMSAgent_-WatchPath>
    </watchPaths>
  </scm>
  <quietPeriod>15</quietPeriod>
  <canRoam>true</canRoam>
  <disabled>false</disabled>
  <blockBuildWhenDownstreamBuilding>false</blockBuildWhenDownstreamBuilding>
  <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
  <triggers>
    <hudson.triggers.SCMTrigger>
      <spec>@hourly</spec>
      <ignorePostCommitHooks>false</ignorePostCommitHooks>
    </hudson.triggers.SCMTrigger>
  </triggers>
  <concurrentBuild>false</concurrentBuild>
  <builders>
    <hudson.tasks.Shell>
      <command>
bob-jenkins --base-uri $WMS --repo scc/bob-plans --plan centos-6n/proddef.bob
</command>
    </hudson.tasks.Shell>
  </builders>
  <publishers>
    <hudson.tasks.junit.JUnitResultArchiver>
      <testResults>output/tests/junit.xml</testResults>
      <keepLongStdio>false</keepLongStdio>
      <testDataPublishers />
    </hudson.tasks.junit.JUnitResultArchiver>
    <hudson.plugins.descriptionsetter.DescriptionSetterPublisher plugin="description-setter">
      <regexp>Revisions built: (.*)</regexp>
      <regexpForFailed />
      <description>\1</description>
      <setForMatrix>false</setForMatrix>
    </hudson.plugins.descriptionsetter.DescriptionSetterPublisher>
    <hudson.tasks.BuildTrigger>
      <childProjects>rbuilder-@BRANCH@_pcreator, rbuilder-@BRANCH@_pcreator,rbuilder-@BRANCH@_rbuild, rbuilder-@BRANCH@_rbuild</childProjects>
      <threshold>
        <name>SUCCESS</name>
        <ordinal>0</ordinal>
        <color>BLUE</color>
        <completeBuild>true</completeBuild>
      </threshold>
    </hudson.tasks.BuildTrigger>
  </publishers>
  <buildWrappers />
</project>
