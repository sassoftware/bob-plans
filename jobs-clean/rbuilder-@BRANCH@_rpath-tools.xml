<?xml version='1.0' encoding='utf-8'?>
<project>
  <actions />
  <description />
  <keepDependencies>false</keepDependencies>
  <properties />
  <scm class="org.jenkinsci.plugins.git_wms.GitWMSAgent">
    <path>gerrit-pdt/appengine/appengine</path>
    <branch>@BRANCH@</branch>
    <watchPaths>
      <org.jenkinsci.plugins.git__wms.GitWMSAgent_-WatchPath>
        <watchRepo>gerrit-pdt/appengine/rpath-tools</watchRepo>
        <watchPath />
      </org.jenkinsci.plugins.git__wms.GitWMSAgent_-WatchPath>
    </watchPaths>
  </scm>
  <quietPeriod>15</quietPeriod>
  <canRoam>true</canRoam>
  <disabled>false</disabled>
  <blockBuildWhenDownstreamBuilding>false</blockBuildWhenDownstreamBuilding>
  <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
  <triggers>
    <hudson.triggers.SCMTrigger>
      <spec>@hourly</spec>
      <ignorePostCommitHooks>false</ignorePostCommitHooks>
    </hudson.triggers.SCMTrigger>
  </triggers>
  <concurrentBuild>false</concurrentBuild>
  <builders>
    <hudson.tasks.Shell>
      <command>
bob-jenkins --base-uri $WMS --repo gerrit-pdt/appengine/bob-plans --plan centos-6n/rpath-tools.bob
</command>
    </hudson.tasks.Shell>
  </builders>
  <publishers>
    <hudson.plugins.descriptionsetter.DescriptionSetterPublisher plugin="description-setter">
      <regexp>Revisions built: (.*)</regexp>
      <regexpForFailed />
      <description>\1</description>
      <setForMatrix>false</setForMatrix>
    </hudson.plugins.descriptionsetter.DescriptionSetterPublisher>
    <hudson.tasks.BuildTrigger>
      <childProjects>rbuilder-@BRANCH@_group-devimage-appliance, rbuilder-@BRANCH@_group-entsrv, rbuilder-@BRANCH@_group-rbuilder, rbuilder-@BRANCH@_group-rpath-packages, rbuilder-@BRANCH@_group-updateservice, rbuilder-@BRANCH@_rpath-repeater, @BRANCH@_centos-6n-common_group-rpath-packages, rbuilder-@BRANCH@_group-devimage-appliance, rbuilder-@BRANCH@_group-entsrv, rbuilder-@BRANCH@_group-rbuilder, rbuilder-@BRANCH@_group-rpath-packages, rbuilder-@BRANCH@_group-updateservice</childProjects>
      <threshold>
        <name>SUCCESS</name>
        <ordinal>0</ordinal>
        <color>BLUE</color>
        <completeBuild>true</completeBuild>
      </threshold>
    </hudson.tasks.BuildTrigger>
  </publishers>
  <buildWrappers />
</project>
