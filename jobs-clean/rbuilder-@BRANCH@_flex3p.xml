<?xml version='1.0' encoding='utf-8'?>
<project>
  <actions />
  <description />
  <keepDependencies>false</keepDependencies>
  <properties />
  <quietPeriod>15</quietPeriod>
  <canRoam>true</canRoam>
  <disabled>false</disabled>
  <blockBuildWhenDownstreamBuilding>false</blockBuildWhenDownstreamBuilding>
  <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
  <triggers>
    <hudson.triggers.SCMTrigger>
      <spec>@hourly</spec>
      <ignorePostCommitHooks>false</ignorePostCommitHooks>
    </hudson.triggers.SCMTrigger>
  </triggers>
  <concurrentBuild>false</concurrentBuild>
  <builders>
    <hudson.tasks.Shell>
      <command>bob-jenkins --base-uri $WMS --repo scc/bob-plans --plan centos-6n/flex3p.bob</command>
    </hudson.tasks.Shell>
  </builders>
  <publishers>
    <hudson.plugins.blame__upstream__commiters.BlameUpstreamCommitersPublisher plugin="blame-upstream-commiters@1.2">
      <sendToIndividuals>false</sendToIndividuals>
    </hudson.plugins.blame__upstream__commiters.BlameUpstreamCommitersPublisher>
    <hudson.plugins.descriptionsetter.DescriptionSetterPublisher plugin="description-setter@1.8">
      <regexp>Revisions built: (.*)</regexp>
      <regexpForFailed />
      <description>\1</description>
      <setForMatrix>false</setForMatrix>
    </hudson.plugins.descriptionsetter.DescriptionSetterPublisher>
    <hudson.tasks.BuildTrigger>
      <childProjects>rbuilder-@BRANCH@_flexlibs</childProjects>
      <threshold>
        <name>SUCCESS</name>
        <ordinal>0</ordinal>
        <color>BLUE</color>
        <completeBuild>true</completeBuild>
      </threshold>
    </hudson.tasks.BuildTrigger>
  </publishers>
  <buildWrappers>
    <hudson.plugins.timestamper.TimestamperBuildWrapper plugin="timestamper@1.5.7" />
  </buildWrappers>
  <scm class="org.jenkinsci.plugins.git_wms.GitWMSAgent">
    <path>scc/appengine</path>
    <branch>@BRANCH@</branch>
    <watchPaths>
      <org.jenkinsci.plugins.git__wms.GitWMSAgent_-WatchPath>
        <watchRepo>scc/flex3p</watchRepo>
        <watchPath />
      </org.jenkinsci.plugins.git__wms.GitWMSAgent_-WatchPath>
    </watchPaths>
  </scm>
</project>
