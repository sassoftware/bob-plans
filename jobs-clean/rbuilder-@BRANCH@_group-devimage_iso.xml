<?xml version='1.0' encoding='utf-8'?>
<project>
  <actions />
  <description />
  <keepDependencies>false</keepDependencies>
  <properties />
  <scm class="hudson.plugins.git.GitSCM" plugin="git@2.0.3">
    <configVersion>2</configVersion>
    <userRemoteConfigs>
      <hudson.plugins.git.UserRemoteConfig>
        <url>git://scc.unx.sas.com/bob-plans.git</url>
      </hudson.plugins.git.UserRemoteConfig>
    </userRemoteConfigs>
    <branches>
      <hudson.plugins.git.BranchSpec>
        <name>*/@BRANCH@</name>
      </hudson.plugins.git.BranchSpec>
    </branches>
    <doGenerateSubmoduleConfigurations>false</doGenerateSubmoduleConfigurations>
    <submoduleCfg class="list" />
    <extensions>
      <hudson.plugins.git.extensions.impl.CleanCheckout />
    </extensions>
  </scm>
  <canRoam>true</canRoam>
  <disabled>true</disabled>
  <blockBuildWhenDownstreamBuilding>false</blockBuildWhenDownstreamBuilding>
  <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
  <triggers />
  <concurrentBuild>false</concurrentBuild>
  <builders>
    <hudson.tasks.Shell>
      <command>
RBUILD="rbuild --config-file ${WORKSPACE}/rbuildrc"
rm -rf rbuild
mkdir rbuild
cd rbuild
$RBUILD init newton.eng.rpath.com@sas:rba-@BRANCH@-devimage
cd */Release
$RBUILD build images 'DevImage @BRANCH@ x86_64 ISO'

cd ${WORKSPACE}
PATH=/usr/local/bin:$PATH ./image-scripts/jenkins.sh newton.eng.rpath.com@sas:rba-@BRANCH@-devimage /mnt/rpath/linux/auto
</command>
    </hudson.tasks.Shell>
  </builders>
  <publishers />
  <buildWrappers>
    <hudson.plugins.timestamper.TimestamperBuildWrapper plugin="timestamper@1.5.8" />
  </buildWrappers>
</project>
