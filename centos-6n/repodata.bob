includeConfigFile ../requires/conary.conf
includeConfigFile ../requires/xmllib.conf
includeConfigFile common.conf

macros version 0.1
scm repodata wms scc/repodata %(rbuilder_forest)s

target repodata-test
target repodata


[target:repodata]
version %(version)s.%(hg)s
scm repodata
flavor_set x86_64
sourceTree repodata recipes/centos-6n/repodata

[target:repodata-test]
version %(version)s.%(hg)s
scm repodata
flavor_set x86_64
sourceTree repodata recipes/centos-6n/repodata-test
