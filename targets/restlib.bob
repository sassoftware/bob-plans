includeConfigFile ../requires/conary.conf
includeConfigFile ../config/rbuilder.conf

macros version 0.1
hg restlib http://scc.eng.rpath.com/products/rbuilder/%(rbuilder_forest)s/restlib

target restlib
target restlib-test

[target:restlib]
version %(version)s.%(hg)s
sourceTree restlib recipes/rpl-2/restlib
hg restlib
flavor_set plain

[target:restlib-test]
version %(version)s.%(hg)s
sourceTree restlib recipes/rpl-2/restlib-test
hg restlib
flavor_set plain
